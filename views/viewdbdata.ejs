<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" type="text/css" href="style/viewDataStyle.css">
<title>View DB Data</title>

</head>
<body>

<center><h1>Firestore DB Data</h1></center>

<table id="customers">






  <tr>
    <th>ID</th>
    <th>Created At</th>
    <th>Name</th>
    <th>Email</th>
    <th>Message</th>
  </tr>


  <% 
  data.forEach(item => {
      %>

      <tr>
          <td><%= item.id %></td>
          <td><%= new Date(item.createdAt) %></td>
          <td><%= item.name %></td>
          <td><%= item.email %></td>
          <td><%= item.message %></td>
        </tr>

        <% 
    });
  
  %>
  
  </table>


  <br><br>
  <center>
    <a href="/formpage">
        <button class="button button2">Add Data Again</button>
    </a>
  </center>

</body>
</html>


